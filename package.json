{
  "name": "lib-poc",
  "version": "0.0.1",
  "scripts": {
    "build:ts": "tsc --build tsconfig.json",
    "rollup:button": "rollup -c src/button/rollup.config.js",
    "rollup:pagination": "rollup -c src/pagination/rollup.config.js",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn build:ts && yarn rollup:button && yarn rollup:pagination"
  },
  "peerDependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@types/react": "17.0.11",
    "rimraf": "3.0.2",
    "rollup": "2.51.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "tslib": "2.3.0",
    "typescript": "4.3.2"
  }
}
